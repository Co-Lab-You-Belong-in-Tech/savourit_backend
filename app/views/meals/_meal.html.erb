<div id="<%= dom_id meal %>">
  <p>
    <strong>Name:</strong>
    <%= meal.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= meal.price %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= meal.description %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= meal.photo %>
  </p>

  <p>
    <strong>Rating:</strong>
    <%= meal.rating %>
  </p>

  <p>
    <strong>User ratings total:</strong>
    <%= meal.user_ratings_total %>
  </p>

  <p>
    <strong>Restaurant:</strong>
    <%= meal.restaurant_id %>
  </p>

</div>
